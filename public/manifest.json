{
  "manifest_version": 2,
  "name": "Extension Name",
  "version": "0.1",
  "description": "Some Description",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["static/js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["static/js/content.js"],
      "matches": ["http://*.github.com/*", "https://*.github.com/*"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "192": "logo192.png"
  },
  "browser_action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}
